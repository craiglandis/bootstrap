{
    "blockman.n04ColorComboPreset": "Lobelia (Gradients)",
    "cSpell.enableFiletypes": [
        "powershell"
    ],
    "cSpell.language": "en",
	"debug.inlineValues": true,
    "debug.openDebug": "neverOpen",    
    "diffEditor.wordWrap": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeLens": false,
    "editor.fontSize": 24,
    "editor.formatOnType": true,
    "editor.glyphMargin": false,
    "editor.links": false,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestions": null,
    "editor.renderIndentGuides": false,
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "log.error",
                "settings": {
                    "foreground": "#af1f1f",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "log.warning",
                "settings": {
                    "foreground": "#f4ad42",
                    "fontStyle": ""
                }
            }
        ]
    },
    "editor.wordWrap": "off",
    "extensions.ignoreRecommendations": false,
    "files.autoSave": "afterDelay",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.postCommitCommand": "sync",
    "json.maxItemsComputed": 30000,
    "logFileHighlighter.customPatterns": [
        {
            "pattern": "Verbose",
            "foreground": "green"
        },
        {
            "pattern": "E/\\w+",
            "foreground": "#af1f1f"
        }
    ],
    "powershell.codeFormatting.autoCorrectAliases": true,	
    "powershell.codeFormatting.openBraceOnSameLine": false,
    "powershell.codeFormatting.preset": "Allman",
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.useCorrectCasing": true,
	"powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "powershell.codeFormatting.whitespaceInsideBrace": false,
    "powershell.enableProfileLoading": false,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.pester.useLegacyCodeLens": false,
    "powershell.powerShellDefaultVersion": "PowerShell Preview (x64)",
	"redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontSize": 24,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#ffffff",
        "terminal.background": "#012456",
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f"
    },
    "workbench.colorTheme": "Visual Studio 2019 Dark",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}